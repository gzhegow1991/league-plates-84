  "minimum-stability": "dev",
  "prefer-stable": true,
  
  "require": {
    "php": "^7.3|^8.0"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.0",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.0"
  },
  
  "autoload": {
    "psr-4": {
      "League\\Plates\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "League\\Plates\\Tests\\": "tests/"
    }
  },
  
  "scripts": {
    "test": "phpunit --testdox --colors=always",
    "docs": "hugo -s doc server"
  },
  
  "config": {
    "optimize-autoloader": true,
    
    "sort-packages": false
  },
  
  "extra": {
    "version": "3.6.1"
  }
}
